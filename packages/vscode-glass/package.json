{
  "name": "vscode-glass",
  "version": "0.4.0",
  "displayName": "Glass",
  "description": "Language support for Glass",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/foundation-ui/vscode-glass.git",
    "directory": "packages/vscode-glass"
  },
  "publisher": "foundation",
  "private": true,
  "engines": {
    "vscode": ">=1.0.0"
  },
  "main": "out/extension.js",
  "keywords": [
    "glass",
    "foundation",
    "gpt",
    "llm"
  ],
  "icon": "assets/glass.png",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:glass",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "vsce": {
    "dependencies": false
  },
  "scripts": {
    "build": "esbuild extension=./src/extension.js language-server=@glass-lang/language-server --bundle --platform=node --target=node16 --external:vscode --outdir=out",
    "build:debug": "npm run copy-libs && npm run build -- --sourcemap",
    "copy-libs": "cpy '../../node_modules/typescript/lib/lib.*.d.ts' out/",
    "vscode:prepublish": "npm run copy-libs && npm run build -- --minify"
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "@vscode/vsce": "^2.0.0",
    "cpy-cli": "^4.0.0",
    "esbuild": "^0.16.0",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-mdx": "^2.0.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "ovsx": "^0.8.0",
    "vscode-languageclient": "^8.0.0"
  },
  "contributes": {
    "configuration": [
      {
        "title": "Glass",
        "properties": {
          "glass.language": {
            "type": "string",
            "default": "typescript",
            "enum": [
              "typescript",
              "javascript",
              "python"
            ],
            "description": "Language to transpile .glass files to"
          },
          "glass.generateTests": {
            "type": "boolean",
            "default": false,
            "description": "Whether to generate test files during transpilation"
          },
          "glass.generateClient": {
            "type": "boolean",
            "default": true,
            "description": "Whether to generate API client during transpilation"
          },
          "glass.defaultTemperature": {
            "type": "number",
            "default": 0,
            "description": "Default temperature to use for model requests"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "glass.transpileCurrentFile",
        "title": "Glass: Transpile current file"
      },
      {
        "command": "glass.transpileAll",
        "title": "Glass: Transpile all"
      }
    ],
    "languages": [
      {
        "id": "glass",
        "aliases": [
          "Glass"
        ],
        "extensions": [
          ".glass"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "tsconfig.json",
        "url": "./tsconfig.schema.json"
      }
    ],
    "grammars": [
      {
        "language": "glass",
        "scopeName": "text.html.markdown.jsx",
        "path": "./syntaxes/glass.tmLanguage.json",
        "embeddedLanguages": {
          "source.js": "javascript",
          "source.js.jsx": "javascriptreact",
          "meta.tag.js": "jsx-tags",
          "meta.tag.without-attributes.js": "jsx-tags",
          "meta.embedded.block.frontmatter": "yaml",
          "meta.embedded.block.css": "css",
          "meta.embedded.block.ini": "ini",
          "meta.embedded.block.java": "java",
          "meta.embedded.block.lua": "lua",
          "meta.embedded.block.makefile": "makefile",
          "meta.embedded.block.perl": "perl",
          "meta.embedded.block.r": "r",
          "meta.embedded.block.ruby": "ruby",
          "meta.embedded.block.php": "php",
          "meta.embedded.block.sql": "sql",
          "meta.embedded.block.vs_net": "vs_net",
          "meta.embedded.block.html": "html",
          "meta.embedded.block.xml": "xml",
          "meta.embedded.block.xsl": "xsl",
          "meta.embedded.block.yaml": "yaml",
          "meta.embedded.block.dosbatch": "dosbatch",
          "meta.embedded.block.clojure": "clojure",
          "meta.embedded.block.coffee": "coffee",
          "meta.embedded.block.c": "c",
          "meta.embedded.block.cpp": "cpp",
          "meta.embedded.block.diff": "diff",
          "meta.embedded.block.dockerfile": "dockerfile",
          "meta.embedded.block.go": "go",
          "meta.embedded.block.groovy": "groovy",
          "meta.embedded.block.pug": "jade",
          "meta.embedded.block.javascript": "javascript",
          "meta.embedded.block.javascriptreact": "javascriptreact",
          "meta.embedded.block.json": "json",
          "meta.embedded.block.less": "less",
          "meta.embedded.block.objc": "objc",
          "meta.embedded.block.scss": "scss",
          "meta.embedded.block.perl6": "perl6",
          "meta.embedded.block.powershell": "powershell",
          "meta.embedded.block.python": "python",
          "meta.embedded.block.rust": "rust",
          "meta.embedded.block.scala": "scala",
          "meta.embedded.block.shellscript": "shellscript",
          "meta.embedded.block.typescript": "typescript",
          "meta.embedded.block.typescriptreact": "typescriptreact",
          "meta.embedded.block.csharp": "csharp",
          "meta.embedded.block.fsharp": "fsharp"
        }
      }
    ]
  },
  "dependencies": {
    "camelcase": "^7.0.1",
    "mdast-util-frontmatter": "^1.0.1",
    "mdast-util-to-markdown": "^1.5.0",
    "micromark-extension-frontmatter": "^1.0.1",
    "prettier": "^2.8.4"
  }
}
